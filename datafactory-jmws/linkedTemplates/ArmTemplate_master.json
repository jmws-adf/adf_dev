{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory-jmws"
		},
		"AzureSqlDatabaseJMWS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseJMWS'"
		},
		"AzureSqlDatabaseMPEP1_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/6f861172-ae0d-4155-8b90-3e57ae8f279c/resourceGroups/rg-jmws/providers/Microsoft.Sql/servers/jmws"
		},
		"AzureSqlDatabaseMPEP1_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory-jmws_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabaseJMWS_connectionString": {
						"value": "[parameters('AzureSqlDatabaseJMWS_connectionString')]"
					},
					"AzureSqlDatabaseMPEP1_properties_privateLinkResourceId": {
						"value": "[parameters('AzureSqlDatabaseMPEP1_properties_privateLinkResourceId')]"
					},
					"AzureSqlDatabaseMPEP1_properties_groupId": {
						"value": "[parameters('AzureSqlDatabaseMPEP1_properties_groupId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}